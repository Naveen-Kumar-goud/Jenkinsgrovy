pipeline{
        agent any
        environment{
                name= "Naveen"
        }
        stages{
              stage('Hello World'){
                  environment{
                        home= "Hyd"
                  }
                  steps{
                        script{
                                birth = "July 15th 1997"
                                echo "$name + $home" 
                        }
                  }
                  
              }
        }
}
